<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:xxoo="http://www.springxoxo.com/schema/beecho"
       xsi:schemaLocation="
     http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
     http://www.springxoxo.com/schema/beecho http://www.springxoxo.com/schema/beecho.xsd"
       default-autowire="byName">

    <xxoo:xxoo userName="春哥" email="1032578874@qq.com"/>
    <xxoo:registry address="192.168.1.1:2181" protocol="zookeeper"/><!--注册中心地址-->
    <xxoo:protocol name="netty"/> <!--协议类型-->
    <xxoo:service service="com.beecho.springxoxo.example.UserService" ref="userService" protocol="netty" group="test"/> <!--提供接口-->

    <bean id="userService" class="com.beecho.springxoxo.example.UserServiceImpl"/> <!--接口的实现-->

    <!--<bean class="com.beecho.springxoxo.annotation.AnnotationContext"/>-->
</beans>